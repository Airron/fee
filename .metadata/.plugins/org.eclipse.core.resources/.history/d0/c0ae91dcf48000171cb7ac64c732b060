package com.feecreate.pojo;

public class Feehospital {
    private Integer hospitalid;

    private Integer hospitalareaid;

    private String hospitalname;

    private String hospitalshortname;

    public Integer getHospitalid() {
        return hospitalid;
    }

    public void setHospitalid(Integer hospitalid) {
        this.hospitalid = hospitalid;
    }

    public Integer getHospitalareaid() {
        return hospitalareaid;
    }

    public void setHospitalareaid(Integer hospitalareaid) {
        this.hospitalareaid = hospitalareaid;
    }

    public String getHospitalname() {
        return hospitalname;
    }

    public void setHospitalname(String hospitalname) {
        this.hospitalname = hospitalname == null ? null : hospitalname.trim();
    }

    public String getHospitalshortname() {
        return hospitalshortname;
    }

    public void setHospitalshortname(String hospitalshortname) {
        this.hospitalshortname = hospitalshortname == null ? null : hospitalshortname.trim();
    }
}